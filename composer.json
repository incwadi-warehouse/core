{
  "name": "incwadi/core",
  "description": "incwadi is a book database to manage your books.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "baldeweg/extra-bundle": "0.2.1",
    "doctrine/doctrine-bundle": "2.2.3",
    "doctrine/doctrine-migrations-bundle": "3.0.2",
    "doctrine/orm": "2.8.2",
    "gesdinet/jwt-refresh-token-bundle": "0.10.1",
    "lexik/jwt-authentication-bundle": "2.11.2",
    "nelmio/cors-bundle": "2.1.0",
    "sensio/framework-extra-bundle": "6.1.0",
    "symfony/asset": "5.2.3",
    "symfony/console": "5.2.3",
    "symfony/dotenv": "5.2.3",
    "symfony/expression-language": "5.2.3",
    "symfony/flex": "1.12.2",
    "symfony/form": "5.2.3",
    "symfony/mime": "5.2.3",
    "symfony/monolog-bundle": "3.6.0",
    "symfony/security-bundle": "5.2.3",
    "symfony/serializer": "5.2.3",
    "symfony/validator": "5.2.3",
    "symfony/yaml": "5.2.3"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.4.0",
    "friendsofphp/php-cs-fixer": "2.18.2",
    "mikey179/vfsstream": "1.6.8",
    "phploc/phploc": "7.0.2",
    "symfony/browser-kit": "5.2.3",
    "symfony/debug-bundle": "5.2.3",
    "symfony/maker-bundle": "1.30.2",
    "symfony/phpunit-bridge": "5.2.3",
    "symfony/stopwatch": "5.2.4",
    "symfony/var-dumper": "5.2.5"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Incwadi\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Incwadi\\Core\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.2.*"
    }
  }
}
